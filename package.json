{
  "name": "knowledgeowl-angular",
  "description": "KnlwledgeOwl widget integration for Angular",
  "author": "https://github.com/scheduleonce/knowledgeowl-angular/graphs/contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/scheduleonce/knowledgeowl-angular"
  },
  "license": "MIT",
  "version": "0.1.9",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint --format stylish",
    "e2e": "ng e2e",
    "prettier": "prettier --list-different \"**/*.{ts,js,css,scss,md,html}\"",
    "fix:prettier": "prettier --write \"**/*.{ts,js,css,scss,md,html}\"",
    "npm-pack": "cd dist/knowledgeowl-angular && npm pack",
    "package": "npm run build && npm run npm-pack && cp README.md dist/knowledgeowl-angular/",
    "publish": "cd dist/knowledgeowl-angular && npm publish",
    "test:lint": "eslint --ext .ts src/lib",
    "fix:lint": "eslint --ext .ts src/lib --fix"
  },
  "dependencies": {
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cdk": "7.3.0",
    "@angular/cli": "9.1.12",
    "@angular/common": "9.1.12",
    "@angular/compiler": "9.1.12",
    "@angular/compiler-cli": "9.1.12",
    "@angular/core": "9.1.12",
    "@angular/language-service": "9.1.12",
    "@angular/platform-browser": "9.1.12",
    "@angular/platform-browser-dynamic": "9.1.12",
    "@oncehub/eslint-config": "^1.1.1",
    "@types/jasmine": "2.8.15",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.11.1",
    "codelyzer": "5.1.2",
    "eslint": "^7.8.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "5.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-junit-reporter": "1.2.0",
    "ng-packagr": "9.0.0",
    "prettier": "1.16.3",
    "protractor": "7.0.0",
    "rxjs": "6.6.3",
    "ts-node": "7.0.1",
    "typescript": "3.8.3",
    "zone.js": "0.10.2"
  }
}
