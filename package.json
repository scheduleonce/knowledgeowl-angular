{
  "name": "knowledgeowl-angular",
  "description": "KnlwledgeOwl widget integration for Angular",
  "author": "https://github.com/scheduleonce/knowledgeowl-angular/graphs/contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/scheduleonce/knowledgeowl-angular"
  },
  "license": "MIT",
  "version": "2.0.3",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "npm run test:lint",
    "prettier": "prettier --list-different \"**/*.{ts,js,css,scss,md,html}\"",
    "fix:prettier": "prettier --write \"**/*.{ts,js,css,scss,md,html}\"",
    "npm-pack": "cd dist/knowledgeowl-angular && npm pack",
    "package": "npm run build && npm run npm-pack && cp README.md dist/knowledgeowl-angular/",
    "publish": "cd dist/knowledgeowl-angular && npm publish",
    "test:lint": "ng lint knowledgeowl-angular",
    "fix:lint": "ng lint knowledgeowl-angular --fix"
  },
  "dependencies": {
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular/cli": "11.1.2",
    "@angular/common": "11.1.1",
    "@angular/compiler": "11.1.1",
    "@angular/compiler-cli": "11.1.1",
    "@angular/core": "11.1.1",
    "@angular/platform-browser": "11.1.1",
    "@angular/platform-browser-dynamic": "11.1.1",
    "@types/jasmine": "~3.6.0",
    "jasmine-core": "~3.6.0",
    "karma": "~6.0.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "1.5.4",
    "karma-junit-reporter": "2.0.1",
    "ng-packagr": "11.1.2",
    "prettier": "2.2.1",
    "puppeteer": "^7.0.1",
    "typescript": "4.1.3",
    "zone.js": "0.11.3",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "@angular-eslint/builder": "1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "1.1.0",
    "@angular-eslint/template-parser": "1.1.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0"
  }
}
