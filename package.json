{
  "name": "knowledgeowl-angular",
  "description": "KnlwledgeOwl widget integration for Angular",
  "author": "https://github.com/scheduleonce/knowledgeowl-angular/graphs/contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/scheduleonce/knowledgeowl-angular"
  },
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "npm run test:lint",
    "e2e": "ng e2e",
    "prettier": "prettier --list-different \"**/*.{ts,js,css,scss,md,html}\"",
    "fix:prettier": "prettier --write \"**/*.{ts,js,css,scss,md,html}\"",
    "npm-pack": "cd dist/knowledgeowl-angular && npm pack",
    "package": "npm run build && npm run npm-pack && cp README.md dist/knowledgeowl-angular/",
    "publish": "cd dist/knowledgeowl-angular && npm publish",
    "test:lint": "eslint --ext .ts src/lib",
    "fix:lint": "eslint --ext .ts src/lib --fix"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular/cdk": "^11.1.0",
    "@angular/cli": "11.1.1",
    "@angular/common": "11.1.0",
    "@angular/compiler": "11.1.0",
    "@angular/compiler-cli": "11.1.0",
    "@angular/core": "11.1.0",
    "@angular/language-service": "11.1.0",
    "@angular/platform-browser": "11.1.0",
    "@angular/platform-browser-dynamic": "11.1.0",
    "@oncehub/eslint-config": "^1.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.11.1",
    "codelyzer": "^6.0.0",
    "eslint": "^7.8.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "1.2.0",
    "ng-packagr": "^11.1.2",
    "prettier": "1.16.3",
    "protractor": "~7.0.0",
    "rxjs": "6.6.3",
    "ts-node": "7.0.1",
    "typescript": "4.0.5",
    "zone.js": "0.10.3"
  }
}
