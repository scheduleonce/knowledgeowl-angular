{
  "name": "knowledgeowl-angular",
  "description": "KnlwledgeOwl widget integration for Angular",
  "author": "https://github.com/scheduleonce/knowledgeowl-angular/graphs/contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/scheduleonce/knowledgeowl-angular"
  },
  "license": "MIT",
  "version": "3.0.1-beta.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "npm run test:lint",
    "prettier": "prettier --list-different \"**/*.{ts,js,css,scss,md,html}\"",
    "fix:prettier": "prettier --write \"**/*.{ts,js,css,scss,md,html}\"",
    "npm-pack": "cd dist/knowledgeowl-angular && npm pack",
    "package": "npm run build && npm run npm-pack && cp README.md dist/knowledgeowl-angular/",
    "publish": "cd dist/knowledgeowl-angular && npm publish",
    "test:lint": "ng lint knowledgeowl-angular",
    "fix:lint": "ng lint knowledgeowl-angular --fix"
  },
  "dependencies": {
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.7",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "12.2.7",
    "@angular/common": "12.2.7",
    "@angular/compiler": "12.2.7",
    "@angular/compiler-cli": "12.2.7",
    "@angular/core": "12.2.7",
    "@angular/platform-browser": "12.2.7",
    "@angular/platform-browser-dynamic": "12.2.7",
    "@types/jasmine": "3.9.1",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsdoc": "36.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "3.9.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "karma-junit-reporter": "2.0.1",
    "ng-packagr": "12.2.2",
    "prettier": "2.4.1",
    "puppeteer": "10.4.0",
    "typescript": "4.3.5",
    "zone.js": "0.11.4"
  }
}
